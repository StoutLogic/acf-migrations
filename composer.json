{
    "name": "stoutlogic/acf-migrations",
    "description": "Make field name and key changes to ACF fields, without losing content.",
    "require": {

    },
    "require-dev": {
        "lucatume/wp-browser": "^1.19",
        "johnpbloch/wordpress": "^4.7"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Steve Pfisterer",
            "email": "steve@stoutlogic.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "StoutLogic\\ACF\\Migrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StoutLogic\\ACF\\Migrations\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "wordpress-install-dir": "tests/wordpress"
    },
    "scripts": {
        "wordpress": "cd tests/wordpress; php -S localhost:8000",
        "test": "wpcept run"
    }
}
